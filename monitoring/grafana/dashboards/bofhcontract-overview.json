{
  "dashboard": {
    "id": null,
    "uid": "bofh-overview",
    "title": "BofhContract V2 - Overview Dashboard",
    "tags": ["bofhcontract", "defi", "swaps"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Swaps",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "bofh_total_swaps",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "blue" },
                { "value": 100, "color": "green" },
                { "value": 1000, "color": "yellow" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Success Rate",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "(bofh_successful_swaps / bofh_total_swaps) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 90, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Profit (BNB)",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "bofh_total_profit_bnb",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 6,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 0.001, "color": "yellow" },
                { "value": 0.1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Contract Status",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "bofh_contract_paused",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "value": 0, "text": "ACTIVE" },
              { "value": 1, "text": "PAUSED" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Swap Rate (per minute)",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(bofh_total_swaps[1m]) * 60",
            "refId": "A",
            "legendFormat": "Swaps per minute"
          }
        ],
        "options": {
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "show": true,
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Gas Usage per Swap",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "bofh_gas_used",
            "refId": "A",
            "legendFormat": "Gas Used"
          },
          {
            "expr": "avg_over_time(bofh_gas_used[5m])",
            "refId": "B",
            "legendFormat": "5min Average"
          }
        ],
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "show": true,
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 5
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 250000, "color": "yellow" },
                { "value": 300000, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Price Impact (%)",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "bofh_price_impact_percent",
            "refId": "A",
            "legendFormat": "Price Impact"
          },
          {
            "expr": "avg_over_time(bofh_price_impact_percent[5m])",
            "refId": "B",
            "legendFormat": "5min Average"
          }
        ],
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "show": true,
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 3, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "Profit per Swap (BNB)",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "bofh_profit_bnb",
            "refId": "A",
            "legendFormat": "Profit/Loss"
          }
        ],
        "options": {
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "show": true,
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 6,
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 70
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": -0.01, "color": "red" },
                { "value": 0, "color": "yellow" },
                { "value": 0.001, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "type": "stat",
        "title": "MEV Protection Status",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "bofh_mev_protection_enabled",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "value": 0, "text": "DISABLED" },
              { "value": 1, "text": "ENABLED" }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 1, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "type": "stat",
        "title": "MEV Triggers",
        "gridPos": {
          "x": 6,
          "y": 20,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "bofh_mev_protection_triggers",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 10, "color": "yellow" },
                { "value": 50, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "type": "stat",
        "title": "Blacklisted Pools",
        "gridPos": {
          "x": 12,
          "y": 20,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "bofh_pools_blacklisted_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Swap Path Length Distribution",
        "gridPos": {
          "x": 18,
          "y": 20,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, bofh_swap_path_length_bucket)",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, bofh_swap_path_length_bucket)",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, bofh_swap_path_length_bucket)",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "show": true,
            "placement": "bottom"
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      }
    ]
  }
}
